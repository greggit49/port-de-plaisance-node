{
  "source": {
    "include": ["controllers", "routes", "middleware", "models", "app.js"],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.js$"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "README.md"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
"tutorials": "./tutorials"
    }
  }